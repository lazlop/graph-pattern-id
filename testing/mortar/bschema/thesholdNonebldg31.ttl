@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix ns1: <https://brickschema.org/schema/Brick/ref#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<urn:hpflex/shapes#Building_Electric_Meter_version_1> a brick:Building_Electric_Meter ;
    rdfs:label rdfs:Resource .

<urn:hpflex/shapes#Building_version_1> a brick:Building ;
    brick:area <urn:hpflex/shapes#Resource_version_2> .

<urn:hpflex/shapes#Electric_Meter_version_1> a brick:Electric_Meter ;
    rdfs:label rdfs:Resource ;
    brick:isPointOf <urn:hpflex/shapes#Chiller_version_1> .

<urn:hpflex/shapes#Ontology_version_1> a owl:Ontology ;
    rdfs:label rdfs:Resource ;
    owl:imports rdfs:Resource .

<urn:hpflex/shapes#Chilled_Water_Return_Temperature_Sensor_version_1> a brick:Chilled_Water_Return_Temperature_Sensor ;
    rdfs:label rdfs:Resource ;
    ns1:hasExternalReference <urn:hpflex/shapes#Resource_version_4> .

<urn:hpflex/shapes#Chilled_Water_Return_Temperature_Sensor_version_2> a brick:Chilled_Water_Return_Temperature_Sensor ;
    rdfs:label rdfs:Resource .

<urn:hpflex/shapes#Chilled_Water_Supply_Temperature_Sensor_version_1> a brick:Chilled_Water_Supply_Temperature_Sensor ;
    rdfs:label rdfs:Resource .

<urn:hpflex/shapes#Chilled_Water_Supply_Temperature_Sensor_version_2> a brick:Chilled_Water_Supply_Temperature_Sensor ;
    rdfs:label rdfs:Resource ;
    ns1:hasExternalReference <urn:hpflex/shapes#Resource_version_1> .

<urn:hpflex/shapes#Chiller_version_1> a brick:Chiller ;
    brick:hasPoint <urn:hpflex/shapes#Chilled_Water_Return_Temperature_Sensor_version_1>,
        <urn:hpflex/shapes#Chilled_Water_Return_Temperature_Sensor_version_2>,
        <urn:hpflex/shapes#Chilled_Water_Supply_Temperature_Sensor_version_1>,
        <urn:hpflex/shapes#Chilled_Water_Supply_Temperature_Sensor_version_2>,
        <urn:hpflex/shapes#Damper_Position_Command_version_1> .

<urn:hpflex/shapes#Damper_Position_Command_version_1> a brick:Damper_Position_Command ;
    rdfs:label rdfs:Resource ;
    ns1:hasExternalReference <urn:hpflex/shapes#Resource_version_3> .

<urn:hpflex/shapes#Resource_version_1> ns1:hasTimeseriesId rdfs:Resource .

<urn:hpflex/shapes#Resource_version_2> brick:hasUnits rdfs:Resource ;
    brick:value rdfs:Resource .

<urn:hpflex/shapes#Resource_version_3> ns1:hasTimeseriesId rdfs:Resource .

<urn:hpflex/shapes#Resource_version_4> ns1:hasTimeseriesId rdfs:Resource .

